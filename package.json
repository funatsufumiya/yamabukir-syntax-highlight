{
    "name": "yamabukir-syntax-highlight",
    "displayName": "YamabukiR Syntax Highlight",
    "description": "YamabukiR Syntax Highlight",
    "publisher": "funatsu-fumiya",
    "version": "0.0.2",
    "license": "The Unlicense",
    "repository": {
        "type": "git",
        "url": "https://github.com/funatsufumiya/yamabukir-syntax-highlight.git"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yab",
            "aliases": ["YamabukiR Layout Definition", "yab"],
            "extensions": [".yab"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yab",
            "scopeName": "source.yab",
            "path": "./syntaxes/yab.tmLanguage.json"
        }]
    }
